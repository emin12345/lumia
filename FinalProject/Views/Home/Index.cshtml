@using FinalProject.Entities;
@using FinalProject.ViewModels;
@{
    ViewData["Title"] = "Home Page";
    List<Product> products = ViewBag.NewArrival;
    List<Product> women = ViewBag.Women;
    List<Product> men = ViewBag.Men;
}
@model HomeVM
@if (TempData["Succeeded"]!=null)
{
    <input type="hidden" id="toastrJs" data-succeed="@TempData["Succeeded"]!"></input>
}
@if (TempData["Login"] != null)
{
    <input type="hidden" id="loginJs" login="@TempData["Login"]!"></input>
}
@if (TempData["EmailSender"] != null)
{
    <input type="hidden" id="emailJs" email="@TempData["EmailSender"]!"></input>
}
@if (TempData["Logout"] != null)
{
    <input type="hidden" id="logoutJs" logout="@TempData["Logout"]!"></input>
}
@if (TempData["Register"] != null)
{
    <input type="hidden" id="registerJs" register="@TempData["Register"]!"></input>
}
@if (TempData["Forgot"] != null)
{
    <input type="hidden" id="forgotJs" forgot="@TempData["Forgot"]!"></input>
}
