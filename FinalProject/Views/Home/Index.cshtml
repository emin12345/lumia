        <div class="owl-carousel owl-theme">
            @foreach (Slider slider in Model.sliders)
            {
                <div class="item">
                    <div style="position: absolute;width: 100%;position: absolute;display: flex;flex-direction: column;align-items: center;height: 100%;justify-content: center;" class="desc">
                        <div class="title animate__animated animate__fadeInDown slow">
                            <h6 style="font-size: 13px;
                    font-weight: 400;">
                                New Arrival
                            </h6>
                        </div>
                        <div class="parag animate__animated animate__fadeInLeft slower">
                            <span style="font-weight: 500;display: block;width: 260px;text-align: center;font-size: 41px;font-family: sans-serif;">@slider.Title</span>
                        </div>
                        <p class="animate__animated animate__fadeInRight animate__delay-1s" style="
                    width: 418px;
                    text-align: center;
                    font-size: 13px;
                ">
                            @slider.MiniDesc
                        </p>
                    </div>
                    <img style="height:575px;object-fit:cover" src="./images/slider/@slider.ImagePath" alt="">
                </div>
            }


